<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="action_healthcare_providers_speciality_view" model="ir.actions.act_window">
        <field name="name">Speciality</field>
        <field name="res_model">healthcare.providers.speciality</field>
        <field name="view_mode">list,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a Provider's Speciality in Health Care Center
            </p>
        </field>
    </record>

    <record id="view_healthcare_providers_speciality_list" model="ir.ui.view">
        <field name="name">healthcare.providers.speciality.list</field>
        <field name="model">healthcare.providers.speciality</field>
        <field name="arch" type="xml">
            <list string="Speciality" editable="bottom" sample="1">
                <field name="name"/>
            </list>
        </field>
    </record>
</odoo>