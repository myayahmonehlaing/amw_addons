<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_healthcare_images" model="ir.sequence">
        <field name="name">Healthcare Images</field>
        <field name="code">healthcare.images.code</field>
        <field name="prefix">IM</field>
        <field name="padding">5</field>
        <field name="company_id" eval="True"/>
    </record>

    <record id="seq_healthcare_patient" model="ir.sequence">
        <field name="name">Healthcare Patient</field>
        <field name="code">healthcare.patients.code</field>
        <field name="prefix">PAT</field>
        <field name="padding">5</field>
        <field name="company_id" eval="True"/>
    </record>

    <record id="seq_healthcare_lab" model="ir.sequence">
        <field name="name">Healthcare Labs</field>
        <field name="code">healthcare.labs.code</field>
        <field name="prefix">LB</field>
        <field name="padding">5</field>
        <field name="company_id" eval="True"/>
    </record>

    <record id="seq_healthcare_appointment" model="ir.sequence">
        <field name="name">Healthcare Appointment</field>
        <field name="code">healthcare.appointment.code</field>
        <field name="prefix">A</field>
        <field name="padding">5</field>
        <field name="company_id" eval="True"/>
    </record>

    <record id="seq_healthcare_billing" model="ir.sequence">
        <field name="name">Healthcare Billing</field>
        <field name="code">healthcare.billing.code</field>
        <field name="prefix">B</field>
        <field name="padding">5</field>
        <field name="company_id" eval="True"/>
    </record>

</odoo>
